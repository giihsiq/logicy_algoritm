algoritmo "soma"
// Função :
// Autor :
// Data : 13/08/2023
// Seção de Declarações 
var

   x, y : inteiro
   soma : inteiro

inicio

   soma <- (x + y)
   
   escreva("Digite o valor de X: ")
   leia(x)
   escreva("Digite o valor de Y: ")
   leia(y)
   
   soma <- (x + y)
   escreval("A soma de x + y = ", soma)
      

fimalgoritmo
