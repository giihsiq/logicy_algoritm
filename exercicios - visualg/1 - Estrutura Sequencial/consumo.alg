algoritmo "consumo"
// Função :
// Autor :
// Data : 14/08/2023
// Seção de Declarações 
var

   dp, cg, cm : real

inicio

  escreva("Distancia percorrida: ")
  leia(dp)
  escreva("Combustível gasto: ")
  leia(cg)

  cm <- (dp / cg)
  
  escreval("Consumo medio = ", cm:5:3)

fimalgoritmo
